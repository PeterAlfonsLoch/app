    BEGIN
        MENUITEM "&Options...",                 ID_TOOLS_OPTIONS
        MENUITEM "&Options 2...",               ID_VIEW_OPTIONS
    END
    POPUP "&?"
    BEGIN
        MENUITEM "À &propos vmpLight...",       ID_APP_ABOUT
    END
    POPUP "(&M)"
    BEGIN
        MENUITEM "ID_LANGUAGE_CHANGE",          ID_LANGUAGE_CHANGE
    END
END

