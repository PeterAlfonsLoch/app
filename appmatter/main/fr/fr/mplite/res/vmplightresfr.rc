        MENUITEM "&Bibliothèque multimédia",    ID_VIEW_ALBUM
        MENUITEM SEPARATOR
        POPUP "Codificação"
        BEGIN
            MENUITEM "ID_VIEW_ENCODING",            ID_VIEW_ENCODING
        END
    END
    POPUP "&Lecture"
    BEGIN
        MENUITEM "&Lecture",                    ID_EXECUTE_PLAY
        MENUITEM "&Arrêt",                      ID_EXECUTE_STOP
        MENUITEM "&Pause",                      ID_EXECUTE_PAUSE
